<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pofsoft.t1b1client"
    android:versionCode="2"
    android:versionName="1.1" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.INTERNET" /> 
    
    <application
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        
        android:debuggable="true"
        android:theme="@style/MyTheme"
        android:name=".MatchData">
        <activity
            android:name="com.pofsoft.t1b1client.GetStartList"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity          
            android:name="com.pofsoft.t1b1client.EnterMatchData"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_enter_match_data" >
        </activity>
         <activity
           
            android:name="com.pofsoft.t1b1client.EnterMatchDataFlex"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_enter_match_data" >
        </activity>  
    </application>
	
</manifest>